/* determine the vowels*/
int main() {
  char z [1000];
    int count = 0;
    printf("enter a string\n");
    fgets(z,sizeof(z),stdin);
    
    /* c program is case senstive */
    
    for(int i = 0; z[i] != '\0'; ++i) { if( z[i] == 'A' ||  z[i] == 'a' ||  z[i] == 'E' ||  z[i] == 'e' ||  z[i] == 'I' ||  z[i] == 'i' ||  z[i] == 'O' || z[i] == 'o' ||  z[i] == 'U' ||  z[i] == 'u')
        
        ++count ;
        }
    printf("count %d\n",count);
        
        }
        return 0; 
)
