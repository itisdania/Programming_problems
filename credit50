#include <cs50.h>
#include <stdio.h>
#include <math.h>
#include <string.h>
int main(void)
{
//incomplete but a code for printing off different digits of an int
    
  char sinput[64] = {0, };
  int len =0;
  int count = 0;
  long card; //here the card number will be saved
    printf("Enter a card number: ");
    scanf("%ld", &card);
    printf("%ld\n",card);


    
  long change = card;
  long digitread =card;
   // the do-while is accurate now
   
   
  long replace = digitread;
  int counter = count;
  long firstnum = card;
    do
    {
        /* Remove last digit of 'num' */
    change /= 10;
    count++;
    } 
        
    while(change != 0);
    
     if ((count != 13) && (count != 15) && (count != 16))
     {
     printf("INVALID\n"); }
     else
     {
     firstnum %= 10;
     printf("last digits: %ld\n", firstnum);
       
     do
     {
        
     printf("last digits: %ld\n", digitread);
     replace = digitread /= 10;
     replace %= 10;
     printf("last last last digits: %ld\n", replace);
     counter--;
      }
       
     while( counter-1  != 0 );

//for (int i = 0; i < count; i=i+2)

     printf("Total digits: %d\n", count);
     }
}

